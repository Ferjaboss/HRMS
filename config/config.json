{
  "$schema": "https://developer.microsoft.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {

    "employee-wp-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/employeeWp/EmployeeWpWebPart.js",
          "manifest": "./src/webparts/employeeWp/EmployeeWpWebPart.manifest.json"
        }
      ]
    },
    "certif-wp-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/certifWp/CertifWpWebPart.js",
          "manifest": "./src/webparts/certifWp/CertifWpWebPart.manifest.json"
        }
      ]
    }
  },
  "externals": {},
  "localizedResources": {
    "ControlStrings": "node_modules/@pnp/spfx-controls-react/lib/loc/{locale}.js",
    "EmployeeWpWebPartStrings": "lib/webparts/employeeWp/loc/{locale}.js",
    "CertifWpWebPartStrings": "lib/webparts/certifWp/loc/{locale}.js"
  }
}
